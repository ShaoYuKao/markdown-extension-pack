{
  "name": "markdown-extension-pack",
  "displayName": "markdown-extension-pack",
  "description": "Popular Visual Studio Code extensions for Markdown authoring",
  "version": "0.0.2-0",
  "publisher": "ShaoYuKao",
  "icon": "./images/markdown_icon.png",
  "repository": "https://github.com/ShaoYuKao/markdown-extension-pack",
  "engines": {
    "vscode": "^1.54.0"
  },
  "license": "MIT",
  "keywords": [
    "Markdown"
  ],
  "categories": [
    "Extension Packs",
    "Snippets"
  ],
  "extensionPack": [
    "EditorConfig.EditorConfig",
    "DavidAnson.vscode-markdownlint",
    "yzhang.markdown-all-in-one",
    "bierner.markdown-preview-github-styles",
    "bierner.markdown-emoji",
    "bierner.markdown-yaml-preamble",
    "bierner.markdown-footnotes",
    "bierner.markdown-image-size",
    "bierner.markdown-mermaid",
    "Marchiore.csvtomarkdown",
    "telesoho.vscode-markdown-paste-image",
    "csholmq.excel-to-markdown-table",
    "jerriepelser.copy-markdown-as-html",
    "doggy8088.markdown-extension-pack",
    "jebbs.markdown-extended",
    "onesdev.vscode-paste-image-plus",
    "yzane.markdown-pdf"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "markdown",
        "path": "./snippets/markdown.json"
      }
    ]
  }
}
